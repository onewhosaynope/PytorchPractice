    "import bson, sys\n",
    "import pandas as pd\n",
    "fn = 'surnames.bson'\n",
    "with open(fn,'rb') as f:\n",
    "    data = bson.loads(f.read()) # list of dicts : each row is a dict\n",
    "\n",
    "fn = fn.replace(\"bson\", \"csv\")\n",
    "df = pd.DataFrame(data)\n",
    "df.to_csv(fn)"
